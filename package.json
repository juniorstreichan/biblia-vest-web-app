    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.4",
    "@types/react-router-dom": "^4.3.4",
    "@types/styled-components": "^4.1.16",
